                <Font name="Calibri Bold" size="14.0" />
            </font>
        </Button>
        <Label fx:id="Error" layoutX="69.0" layoutY="333.0" prefHeight="18.0" prefWidth="184.0" textFill="DARKORANGE">
            <font>
                <Font name="Calibri Bold" size="15.0" />
            </font>
        </Label>
      <VBox layoutX="41.0" layoutY="223.0" prefHeight="25.0" prefWidth="228.0">
         <children>
              <TextField fx:id="Email" prefHeight="30.0" prefWidth="0.0" promptText="Email" styleClass="text_field" stylesheets="@../style/TextField.css">
                  <font>
                      <Font name="Calibri Bold" size="15.0" />
                  </font>
              </TextField>
         </children>
      </VBox>
    </children>
</AnchorPane>
